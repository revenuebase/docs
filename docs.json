{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "RevenueBase",
  "colors": {
    "primary": "#00B5B0",
    "light": "#2DC9C4",
    "dark": "#0F537E"
  },
  "background": {
    "color": {
      "light": "#FFFFFF",
      "dark": "#091220"
    }
  },
  "favicon": "/favicon.svg",
  "fonts": {
    "heading": {
      "family": "Fraunces",
      "weight": 400
    },
    "body": {
      "family": "DM Sans"
    }
  },
  "api": {
    "playground": {
      "display": "interactive"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Getting Started",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "getting-started/quickstart-api",
              "getting-started/quickstart-data-feeds",
              "getting-started/quickstart-gigasheet"
            ]
          }
        ]
      },
      {
        "tab": "Dynamic Data Feeds",
        "groups": [
          {
            "group": "Dynamic Data Feeds",
            "pages": [
              "data-feeds/overview",
              "data-feeds/verification-summaries"
            ]
          },
          {
            "group": "Tables & Schema",
            "pages": [
              "data-feeds/contacts",
              "data-feeds/companies",
              "data-feeds/insights",
              "data-feeds/historical-experience"
            ]
          },
          {
            "group": "Pre-Joined Tables",
            "pages": [
              "data-feeds/pre-joined-overview",
              "data-feeds/people-companies",
              "data-feeds/people-companies-insights"
            ]
          },
          {
            "group": "Joining Tables",
            "pages": [
              "data-feeds/joining-overview"
            ]
          },
          {
            "group": "Building Lists",
            "pages": [
              "data-feeds/company-keyword-search"
            ]
          }
        ]
      },
      {
        "tab": "Gigasheet Access",
        "groups": [
          {
            "group": "Gigasheet Access",
            "pages": [
              "gigasheet/overview"
            ]
          },
          {
            "group": "Setup & access",
            "pages": [
              "gigasheet/accessing-your-data",
              "gigasheet/adding-users"
            ]
          },
          {
            "group": "Search, filter & views",
            "pages": [
              "gigasheet/filtering",
              "gigasheet/saving-views",
              "gigasheet/sharing-data"
            ]
          },
          {
            "group": "Export & enrichment",
            "pages": [
              "gigasheet/exporting",
              "gigasheet/exporting-hubspot",
              "gigasheet/enrichment"
            ]
          }
        ]
      },
      {
        "tab": "API Reference & Playground",
        "openapi": "openapi.json",
        "groups": [
          {
            "group": "API Reference & Playground",
            "pages": [
              "api-reference/overview",
              "api-reference/authentication",
              "api-reference/rate-limits"
            ]
          },
          {
            "group": "User Operations",
            "pages": [
              "GET /v1/credits",
              "GET /v1/new-api-key"
            ]
          },
          {
            "group": "Email Verification",
            "pages": [
              "api-reference/email-verification-overview",
              "POST /v1/process-email",
              "POST /v1/batch-upload",
              "POST /v1/batch-process-email",
              "POST /v1/batch-process-email-status",
              "GET /v1/queued-process",
              "POST /v1/cancel-process",
              "POST /v1/batch-download"
            ]
          },
          {
            "group": "Company Match",
            "pages": [
              "api-reference/company-match-overview",
              "POST /v2/company-resolver/resolve",
              "POST /v2/company-resolver/discovery"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": []
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.png",
    "href": "https://revenuebase.ai"
  },
  "navbar": {
    "links": [
      {
        "label": "Talk to us",
        "href": "https://revenuebase.ai/request-for-demo/"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://app.revenuebase.ai/dashboard"
    }
  },
  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude", "perplexity", "mcp", "cursor", "vscode"]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
